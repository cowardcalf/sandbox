{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true,
    "worker": true
  },
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "import/prefer-default-export": "off",
    "arrow-parens": "off"
  }
}
